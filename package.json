{
  "name": "htmlmodule",
  "version": "0.1.3",
  "description": "DOM assembler",
  "keywords": [
    "xml",
    "dom",
    "html",
    "javascript",
    "module",
    "library"
  ],
  "homepage": "https://aristov.github.io/htmlmodule",
  "bugs": "https://github.com/aristov/htmlmodule/issues",
  "license": "MIT",
  "author": "Vyacheslav Aristov <vv.aristov@gmail.ru> (http://twitter.com/aristov7)",
  "files": [
    "dist/",
    "lib/",
    "shim/",
    "util/"
  ],
  "main": "./lib",
  "directories": {
    "dist": "./dist",
    "lib": "./lib",
    "example": "./docs/lib/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aristov/htmlmodule.git"
  },
  "scripts": {
    "test": "./node_modules/.bin/karma start",
    "start": "DOCS=true ES6=true WATCH=true ./node_modules/.bin/webpack",
    "jsdoc-build": "./node_modules/.bin/documentation build --shallow --output docs/api --format html",
    "jsdoc-serve": "./node_modules/.bin/documentation documentation serve --format html --shallow -w",
    "jsdoc-build-all": "./node_modules/.bin/documentation build --shallow --output docs/api --format html",
    "jsdoc-serve-all": "./node_modules/.bin/documentation serve --shallow --format html -w"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel": "^6.5.2",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "codemirror": "^5.19.0",
    "css-loader": "^0.25.0",
    "documentation": "^4.0.0-beta10",
    "documentation-loader": "^0.1.1-beta",
    "dom4": "^1.8.3",
    "karma": "^1.3.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-sauce-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.0",
    "postcss-loader": "^1.0.0",
    "raw-loader": "^0.5.1",
    "static": "^2.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  },
  "engines": {
    "node": ">=0.6"
  }
}
