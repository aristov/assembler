{
  "name": "htmlmodule",
  "version": "0.5.1",
  "description": "HTML DOM assembler library",
  "keywords": [
    "javascript",
    "dom",
    "a",
    "b",
    "body",
    "button",
    "div",
    "form",
    "h1",
    "html",
    "img",
    "index",
    "input",
    "label",
    "li",
    "main",
    "ol",
    "option",
    "p",
    "select",
    "span",
    "strong",
    "table",
    "ul"
  ],
  "homepage": "https://aristov.github.io/htmlmodule",
  "bugs": "https://github.com/aristov/htmlmodule/issues",
  "license": "MIT",
  "author": "Vyacheslav Aristov <vv.aristov@gmail.ru> (http://twitter.com/aristov7)",
  "files": [
    "lib/",
    "shim/"
  ],
  "main": "./lib",
  "directories": {
    "dist": "./dist",
    "lib": "./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aristov/htmlmodule.git"
  },
  "scripts": {
    "lint": "eslint lib --ignore-pattern *.spec.js --quiet && eslint lib/*.spec.js --env browser,mocha && eslint shim docs",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "mocha": "mocha",
    "karma": "karma start",
    "test-full": "npm run karma && npm test",
    "build": "webpack",
    "webpack": "webpack -w",
    "api": "esdoc"
  },
  "dependencies": {
    "dommodule": "^0.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.0",
    "dom4": "^1.8.5",
    "esdoc": "^1.0.3",
    "esdoc-standard-plugin": "^1.0.0",
    "jsdom": "^11.3.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "sinon": "^4.0.1",
    "webpack": "^3.5.0",
    "whatwg-fetch": "^2.0.3"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
